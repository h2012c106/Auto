<body class="user-select">
  <header class="header">
    <nav class="navbar navbar-default" id="navbar">
      <div class="container">
        <%- include('../components/header'); %>
      </div>
    </nav>
  </header>
  <section class="container">
    <div class="content-wrap">
      <div class="publish-wrapper">
        <form id="publish-form" enctype="multipart/form-data">
          <label>标题</label>
          <input name="title" class="form-control publish-input">
          <label>作者</label>
          <input name="author" class="form-control publish-input">
          <label>摘要</label>
          <input name="quotation" class="form-control publish-input">
          <label>新闻图片</label>
          <label class="form-control btn publish-input pic-upload" for="pic-input">上传图片</label>
          <input name="pic" type="file" accept="image/*" id="pic-input" class="publish-input" style="position:absolute;clip:rect(0 0 0 0);">
          <label>正文</label>
          <textarea name="text" wrap="virtual" class="form-control publish-input"></textarea>
        </form>
        <div class="publish-btn-wrapper">
          <span class="btn btn-default btn-publish" id="news-publish-btn">发表</span>
        </div>
      </div>
    </div>
  </section>
  <footer class="footer">
    <div class="container">
      <%- include('../components/footer'); %>
    </div>
    <div id="gotop"><a class="gotop"></a></div>
  </footer>
  <script src="/js/extern/bootstrap.min.js"></script>
  <script src="/js/extern/jquery.ias.js"></script>
  <script src="/js/extern/scripts.js"></script>
  <script src="/js/extern/bootstrap-dialog.min.js"></script>
  <script src="/js/tools.js"></script>
